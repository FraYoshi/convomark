{
  "name": "convomarkbot",
  "version": "0.1.0",
  "description": "Bookmark your telegram conversations",
  "repository": "https://github.com/matteojoliveau/convomarkbot",
  "author": "Matteo Joliveau <matteojoliveau@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "start": "ts-node -r dotenv/config src/main.ts",
    "lint": "tslint src/**/*.ts",
    "start:prod": "node dist/main.js",
    "start:watch": "nodemon",
    "test": "jest --verbose",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db:rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "db:generate:migration": "ts-node ./node_modules/typeorm/cli.js migration:create -n ",
    "db:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop"
  },
  "dependencies": {
    "apollo-server-fastify": "^2.6.1",
    "date-fns": "^1.30.1",
    "decko": "^1.2.0",
    "fastify": "^2.2.0",
    "fastify-cors": "^2.1.3",
    "fastify-jwt": "^0.10.0",
    "graphql": "^14.2.1",
    "i18n-ts": "^1.0.5",
    "inversify": "^5.0.1",
    "lodash": "^4.17.11",
    "pg": "^7.11.0",
    "pino": "^5.12.2",
    "pino-clf": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "slug": "^1.1.0",
    "telegraf": "^3.30.1",
    "typeorm": "^0.2.18",
    "typescript-optional": "^2.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^12.0.4",
    "@types/oauth2-server": "^3.0.10",
    "@types/pino": "^5.8.6",
    "@types/redis": "^2.8.12",
    "@types/slug": "^0.9.1",
    "dotenv": "^7.0.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.10",
    "pino-pretty": "^2.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.2"
  }
}
